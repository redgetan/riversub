<div class='row'>

  <div id="group_repositories_container" class="col-md-10 col-md-offset-1">
    <h4>Ready for Youtube Integration</h2>
    <ul class="unexported_repositories_container">
      <% @group.unexported_repositories_grouped_by_video.each do |video, repositories| %>
        <li>
          <%= video.name %>
          <ul class='repo_language_container'>
            <% repositories.each do |repo| %>
              <li class='repo_language'>
                <%= repo.language_display %>
                <% if repo.youtube_exported? %>
                  <a href="#">youtube_subtitle_link</a>
                <% else %>
                  <div class="unexported_repository_controls pull-right">
                    |
                    <a href="<%= repo.url %>" class='repo_url'>Preview</a>
                    |
                    <a href="#" class='youtube_export_btn'>Youtube Export</a>
                  </div>
                <% end %>
              </li>
            <% end %>
          </ul>
        </li>
      <% end %>
    </ul>
    <h4>Archives</h2>
    <ul class="exported_repositories_container">
      <% @group.exported_repositories_grouped_by_video.each do |video, repositories| %>
        <li>
          <%= video.name %>
          <ul class='repo_language_container'>
            <% repositories.each do |repo| %>
              <li class='repo_language'>
                <%= repo.language_display %>
                <a href="#">youtube_subtitle_link</a>
              </li>
            <% end %>
          </ul>
        </li>
      <% end %>
    </ul>
  </div>
</div>
  <!-- <%= render 'youtube_integration' %> -->
