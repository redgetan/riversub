<div id="top_container">
</div>

<div id="container">
  <div id="songs">

    <div id="with_sync_files">
      <p>
        Playable
      </p>
      <ul>
        <% @songs_with_sync_files.each do |song| %>
          <li id="<%= song.id %>"><%= link_to song.name, "#", :class => "song" %></li>
        <% end %>
      </ul>
    </div>

    </br>
    </br>

    <div id="no_sync_files">
      <p>
        Add SyncFiles to these Songs
      </p>
      <ul>
        <% @songs_no_sync_files.each do |song| %>
          <li id="<%= song.id %>"><%= link_to song.name, "#", :class => "song" %></li>
        <% end %>
      </ul>
    </div>

    <div id="new">
      <input type="button" name="" value="Add Song" id="add_song_btn"/>
    </div>
  </div>

  <div id="media_container">
    <div id="media">

    </div>

    <div id="media_sources">
    </div>


  </div>

  <div id="sync_container">
    <div id="sync_mode_controls">
    </div>

    <div id="lyrics">
    </div>
  </div>
</div>
